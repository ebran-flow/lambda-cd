                        },
                        "SECRET_KEY": {
                            "Ref": "secretKey"
                        },
                        "STMT_DB_DATABASE": {
                            "Ref": "stmtDbDatabase"
                        },
                        "STMT_DB_HOST": {
                            "Ref": "stmtDbHost"
                        },
                        "STMT_DB_PASSWORD": {
                            "Ref": "stmtDbPassword"
                        },
                        "STMT_DB_USERNAME": {
                            "Ref": "stmtDbUsername"
                        }
                    }
                },
                "Layers": [
                    {
                        "Ref": "PythonPackages"
                    }
                ],
                "VpcConfig": {
                    "SecurityGroupIds": [
                        {
                            "Ref": "lambdaSecurityGroup"
                        }
                    ],
                    "SubnetIds": [
                        {
                            "Ref": "lambdaPrivateSubnetA"
                        },
                        {
                            "Ref": "lambdaPrivateSubnetB"
