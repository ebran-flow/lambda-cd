            "Type": "AWS::Lambda::Function",
            "Properties": {
                "FunctionName": "rmtn_export",
                "Runtime": "python3.8",
                "Role": {
                    "Fn::Sub": [
                        "arn:aws:iam::${AWS::AccountId}:role/${ETLRole}",
                        {
                            "ETLRole": {
                                "Ref": "lambdaRole"
                            }
                        }
                    ]
                },
                "Handler": "rmtn_export.lambda_handler",
                "Code": {
                    "S3Bucket": {
                        "Ref": "artifactsS3"
                    },
                    "S3Key": {
                        "Fn::Sub": "${deployTime}/rmtn_export.zip"
                    }
                },
                "Timeout": 300,
                "MemorySize": 800,
                "Environment": {
                    "Variables": {
                        "ACCESS_KEY": {
                            "Ref": "accessKey"
                        },
                        "APP_ENV": {
                            "Ref": "appEnv"
                        },
                        "APP_URL": {
                            "Ref": "appUrl"
                        },
                        "AWS_ACCOUNT_ID": {
